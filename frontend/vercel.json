{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/angular"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "framework": "angular",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/agendador-frontend",
  "rootDirectory": "frontend"
}
